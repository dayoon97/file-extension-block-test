--  DDL for Index 엔터티1_PK
--------------------------------------------------------

  CREATE UNIQUE INDEX "엔터티1_PK" ON "FILE_EXTENSION_LIST" ("FILE_EXTENSION_CODE") 
  PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SYSTEM" ;
--------------------------------------------------------
--  Constraints for Table FILE_EXTENSION_LIST
--------------------------------------------------------

  ALTER TABLE "FILE_EXTENSION_LIST" ADD CONSTRAINT "엔터티1_PK" PRIMARY KEY ("FILE_EXTENSION_CODE")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SYSTEM"  ENABLE;
  ALTER TABLE "FILE_EXTENSION_LIST" ADD CHECK (FILE_EXTENSION_TYPE IN('고정', '커스텀')) ENABLE;
  ALTER TABLE "FILE_EXTENSION_LIST" MODIFY ("FILE_EXTENSION_NAME" NOT NULL ENABLE);
  ALTER TABLE "FILE_EXTENSION_LIST" MODIFY ("FILE_EXTENSION_CODE" NOT NULL ENABLE);
